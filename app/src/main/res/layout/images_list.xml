            android:layout_marginBottom="2dp"
            android:layout_weight="1"
            app:layout_constraintBottom_toTopOf="@+id/hor25"
            app:layout_constraintEnd_toStartOf="@+id/ver66"
            app:layout_constraintStart_toStartOf="@+id/ver33"
            app:layout_constraintTop_toTopOf="parent"
